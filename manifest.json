{
	"manifest_version" : 2,
	"name": "Jon's Extension",
	"version" : "0.1",
	"default_local" : "en",
	"description" : "just some description here",
	"content_scripts" : [
		{
			"matches" : [
				"<all_urls>"
			],
			"js" : ["jquery.3.3.1.min.js", "content.js"]
		}
	],
	"browser_action": {
		"default_action" : "icon.png"
	},
	"background": {
	  "scripts": ["background.js"]
	}

}